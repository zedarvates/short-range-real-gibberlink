<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">RgibberLink</string>
    <style name="Theme.GibberLink" parent="Theme.Material3.DayNight.NoActionBar">
        
        <item name="colorPrimary">@android:color/holo_blue_bright</item>
        <item name="colorPrimaryVariant">@android:color/holo_blue_dark</item>
        <item name="colorOnPrimary">@android:color/white</item>
        
        <item name="colorSecondary">@android:color/holo_green_light</item>
        <item name="colorSecondaryVariant">@android:color/holo_green_dark</item>
        <item name="colorOnSecondary">@android:color/black</item>
        
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>
    <style name="Theme.GibberLink.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
</resources>